<h1>Pesquisa realizada pelo filtro</h1>
<hr>
<main>
    <div class="container">
        <section class="container">
            <table class="table table-hover myTable">
                <thead>
                    <tr>
                        <th>Nome</th>
                        <th>Cidade</th>
                        <th>Endere√ßo</th>
                        <th>Telefone</th>
                        <th>Data nascimento</th>
                        <th>E-mail</th>
                    </tr>
                </thead>
                <tbody class="container">
                    {{#each dados}}
                    <tr>
                        <td><span>
                                <ion-icon name="person"></ion-icon>
                            </span> {{nm_cliente}}</td>
                        <td>{{nm_cidade}}</td>
                        <td>{{nm_endereco}}</td>
                        <td>{{nm_telefone}}</td>
                        <td>{{dt_nascimento}}</td>
                        <td>{{nm_email}}</td>
                        <td><a
